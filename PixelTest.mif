-- Altera Memory Initialization File (MIF)

DEPTH = 2048;
WIDTH = 16;

ADDRESS_RADIX = HEX;
DATA_RADIX = HEX;

CONTENT
  BEGIN
    [000..7FF] : 0000;  -- Default to NOP

           000 : 0803;  -- 	LOAD BLUECOLOR
           001 : 98B2;  -- 	OUT PXL_All
           002 : F800;  -- REDCOLOR: DW &B1111100000000000
           003 : 001F;  -- BLUECOLOR: DW &B0000000000011111
           004 : 07E0;  -- GREENCOLOR: DW &B0000011111100000
  END;
